pipeline {
    agent any

    stages {
        stage('Git-Checkout') {
            steps {
                echo "Start Git Checkout"
                // Get some code from a GitHub repository
                git credentialsId: '4edc916e-d436-47bf-a0d8-775199bfa17d', url: 'https://github.com/TJayanth/MVC-CRUD.git'
                
            }
        }
        stage('Restore packages'){
               steps{
                   echo "Restore Package"
                   sh 'dotnet restore MVCCRUD.sln'
                 }
        }
        stage('Clean'){
           steps{
               sh 'dotnet clean MVCCRUD.sln --configuration Release'
            }
         }
        stage('Build'){
           steps{
               sh 'dotnet build MVCCRUD.sln --configuration Release --no-restore'
            }
         }

         stage('Release'){
           steps{
               echo "Ready for release"
            }
         }
    }
}
